{{ form_start(form) }}

    <div class="hero-search-input margin-0" style="display:flex; flex-direction:row; gap:20px;">
        <span>Nom</span>
        <input class="form-control form-control-sm" type="text" id='{{ form.aProprietaire.pnom.vars.id }}' name='{{ form.aProprietaire.pnom.vars.full_name }}' value='{{ app.user.aProprietaire.pnom }}' aria-label=".form-control-sm example">
        {% do form.aProprietaire.pnom.setRendered %}
    </div>
    <div class="hero-search-3">
        <div class="hero-search-input second-search margin-1" style="display:flex; flex-direction:row; gap:20px;">
            <span>Prenom</span>
            <input class="form-control form-control-sm" type="text" id='{{ form.aProprietaire.pprenom.vars.id }}' name='{{ form.aProprietaire.pprenom.vars.full_name }}' value='{{ app.user.aProprietaire.pprenom }}' aria-label=".form-control-sm example">
            {% do form.aProprietaire.pprenom.setRendered %}
        </div>
        <div class="hero-search-input second-search margin-2" style="display:flex; flex-direction:row; gap:20px;">
            <span>Telephone</span>
            <input class="form-control form-control-sm" type="text" id='{{ form.aProprietaire.ptele.vars.id }}' name='{{ form.aProprietaire.ptele.vars.full_name }}' value='{{ app.user.aProprietaire.ptele }}' aria-label=".form-control-sm example">
            {% do form.aProprietaire.ptele.setRendered %}
        </div>
        <div class="hero-search-input second-search margin-2" style="display:flex; flex-direction:row; gap:20px;">
            <span>CIN</span>
            <input class="form-control form-control-sm" type="text" id='{{ form.aProprietaire.pcin.vars.id }}' name='{{ form.aProprietaire.pcin.vars.full_name }}' value='{{ app.user.aProprietaire.pcin }}' aria-label=".form-control-sm example">
            {% do form.aProprietaire.pcin.setRendered %}
        </div>
    </div>
    <div class="hero-search-input margin-0" style="display:flex; flex-direction:row;">
        <span>Carte Nationnale</span>
        <input class="form-control" type="File" id='{{ form.aProprietaire.pcinimage.vars.id }}' name='{{ form.aProprietaire.pcinimage.vars.full_name }}' value='{{ app.user.aProprietaire.pcinimage }}' aria-label=".form-control-sm example">
        {% do form.aProprietaire.pcinimage.setRendered %}
        {# {{ form_row(form.aProprietaire.pcinimage) }} #}
    </div>
    <button class="btn btn-primary" data-collection-holder-class="tags">{{ button_label|default('Save') }}</button>
    {# {{ dump(form) }} #}
{{ form_end(form) }}
